        <div class="section__1">
         <button><a href="/admin/addCategory">Add Category</a></button>
            {{#if data}}
           <table id="example" class="display" style="width:100%">
        <thead>
            <tr>
                <th>Category name</th>
                <th>Delete</th>
            </tr>
        </thead>
        <tbody>
{{# each data}}
            <tr>
                <td>{{this.name}}</td>
                <td><a href=""><button class="btn btn-danger">Delete</button></a></td>
            </tr>
 {{/each}}
            </tbody>
            </table>
            {{else}}
