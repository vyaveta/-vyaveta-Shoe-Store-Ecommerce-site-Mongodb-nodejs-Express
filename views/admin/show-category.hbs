<style>
    a{
        text-decoration: none !important;
    }
   .section__1{
    margin-top: 150px;
   }
   .not__blocked{
        padding: 0px 30px 0px 0px !important;
        padding-bottom: -2px;
        height: 40px;
        margin: 0%;
        border: 5px solid rebeccapurple;
        border-radius: 50px;
        cursor: pointer;
        box-shadow: inset 2px 2px 2px 2px rgba(0, 0, 0,.4), inset -2px 2px 2px -2px rgba(0, 0, 0,.5);
   }
   .blocked{
        padding: 0px 0px 0px 30px !important;
        padding-bottom: -2px;
        height: 40px;
        margin: 0%;
        border: 5px solid rebeccapurple;
        border-radius: 50px;
        cursor: pointer;
        box-shadow: inset 2px 2px 2px 2px rgba(0, 0, 0,.4), inset -2px 2px 2px -2px rgba(0, 0, 0,.5);
   }
   .blocked svg,
   .not__blocked svg{
    border-radius: 50px;
        box-shadow:  2px 2px 2px 2px rgba(0, 0, 0,.4),  2px 2px 2px 2px rgba(0, 0, 0,.5);
   }
</style>

